{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "timezone": "Asia/Tokyo",
    "semanticCommits": "enabled",
    "labels": [
        "dependencies"
    ],
    "schedule": [
        "* 9-17 * * mon-fri"
    ],
    "prHourlyLimit": 0,
    "prConcurrentLimit": 0,
    "packageRules": [],
    "regexManagers": [],
    "terraform": {
        "fileMatch": [
            "\\.hcl$"
        ]
    }
}
